{
  "name": "dune-ts",
  "version": "1.1.4",
  "description": "Unofficial Dune Analytics Typescript Client",
  "repository": "git@github.com:r1oga/dune-ts.git",
  "author": {
    "name": "r1oga",
    "email": "me@r1oga.io",
    "url": "https://r1oga.io"
  },
  "license": "MIT",
  "homepage": "https://github.com/r1oga/dune-ts",
  "keywords": [
    "dune",
    "analytics",
    "javascript",
    "typescript",
    "client",
    "ethereum",
    "data"
  ],
  "scripts": {
    "nps": "nps",
    "postpublish": "bash ./postpublish.sh",
    "prepack": "nps build",
    "prepare": "husky install",
    "setup": "pnpm 2> /dev/null || npm i -g pnpm && pnpm i"
  },
  "files": ["dist/**/*"],
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "devDependencies": {
    "@r1oga/eslint-config": "^1.1.8",
    "@r1oga/prettier-config": "^1.1.11",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.18",
    "concurrently": "^7.6.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-chain": "^1.1.6",
    "jest-extended": "^3.2.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-runner-eslint": "^1.1.0",
    "jest-runner-prettier": "^1.0.0",
    "jest-silent-reporter": "^0.5.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "^13.1.0",
    "nps": "^5.10.0",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "yargs": "^17.6.2"
  },
  "engines": {
    "node": ">=18"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.eslintignore"
    }
  },
  "jest-runner-tsc": {
    "tsconfigPath": "./tsconfig.json"
  }
}
